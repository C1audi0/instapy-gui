{
	"name": "instapy-gui",
	"productName": "InstaPy GUI",
	"version": "0.0.1",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"test": "npm run lint",
		"start": "webpack-dev-server --open --config webpack.dev.config.js",
		"build": "webpack --config webpack.prod.config.js",
		"build:electron": "webpack --config webpack.electron.config.js",
		"build:docker": "npm run build:docker:webserver && npm run build:docker:config && npm run build:docker:socket && npm run build:docker:instapy && npm run build:docker:grafana",
		"build:docker:webserver": "docker build -t felixbreuer/instapy-webserver:latest -f conf/webserver.Dockerfile --rm .",
		"build:docker:config": "docker build -t felixbreuer/instapy-service-config:latest -f conf/service_config.Dockerfile --rm .",
		"build:docker:socket": "docker build -t felixbreuer/instapy-service-socket:latest -f conf/service_socket.Dockerfile --rm .",
		"build:docker:instapy": "docker build -t felixbreuer/instapy:latest -f conf/service_instapy.Dockerfile --rm .",
		"build:docker:grafana": "docker build -t felixbreuer/instapy-grafana:latest -f conf/grafana.Dockerfile --rm .",
		"lint": "eslint --config .eslintrc.yml --ext jsx --ext js src",
		"lint:fix": "eslint --fix --config .eslintrc.yml --ext jsx --ext js src"
	},
	"author": "Felix Breuer",
	"license": "GPL3.0",
	"dependencies": {
		"array-move": "^1.0.0",
		"bootstrap": "^4.3.1",
		"classnames": "^2.2.6",
		"fetch-mock": "^7.3.0",
		"jquery": "^3.3.1",
		"linkstate": "^1.1.1",
		"popper.js": "^1.14.7",
		"preact": "^8.4.2",
		"preact-compat": "^3.18.4",
		"react-router-dom": "^4.3.1",
		"slugify": "^1.3.4",
		"unistore": "^3.2.1",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.3.3",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-decorators": "^7.3.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.3.1",
		"autoprefixer": "^9.4.7",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.4",
		"clean-webpack-plugin": "^1.0.1",
		"css-loader": "^2.0.0",
		"eslint": "^5.14.1",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.11.0",
		"postcss-loader": "^3.0.0",
		"purgecss-webpack-plugin": "^1.4.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"webpack": "^4.29.5",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.1.10",
		"webpack-merge": "^4.1.5"
	}
}
